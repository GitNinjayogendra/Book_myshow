 <h1>All cities</h1>

<%= link_to "new city", new_city_path %>
  <table>
    <thead>
      <tr>
        <th>city name</th>
        <th>action </th>
      <tr>
    </thead>

    <tbody>
      <% @cities.each do |city| %>
        <tr>
          <td><%= city.name %></td>
          <td><%= link_to "view", city%></td>
      <% end %>
    </tbody>
  </table>
